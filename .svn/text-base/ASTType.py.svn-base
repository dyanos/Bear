#!/usr/bin/env python
from AST import *
from mangle import *

class ASTType(AST):
  def __init__(self, name, templ, ranks):
    self.name  = name
    self.templ = templ
    self.ranks = ranks

  def convertToManglingName(self):
    return convertType(self)
              
  def printXML(self):
    print "<type>\n<name>"
    self.name.printXML()
    print "</name>"
    if self.templ != None:
      print "<templ>"
      self.templ.printXML()
      print "</templ>"
    if self.ranks != None:
      print "<ranks>"
      self.ranks.printXML()
      print "</ranks>"
    print "</type>"


